	@GenModel(documentation="<p>Whether this <code>FeatureValue</code> is a concrete specification of the bound or initial value of the <code>featureWithValue</code>, or just a default value that may be overridden.</p>")
	!ordered attr types.Boolean[1] isDefault = "false";
