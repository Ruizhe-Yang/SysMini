	!ordered attr types.Boolean[1] isEnd = "false";

	@GenModel(documentation="<p>The <code>Type</code> that is related to this <code>Feature</code> by an <code>EndFeatureMembership</code> in which the <code>Feature</code> is an <code>ownedMemberFeature</code>.</p>")
	@subsets
	@"http://www.omg.org/spec/SysML"
	!ordered volatile transient derived ref Type#ownedEndFeature endOwningType;

	@GenModel(documentation="<p>The <code>ownedSpecializations</code> of this <code>Feature</code> that are <code>FeatureTypings</code>, for which the <code>Feature</code> is the <code>typedFeature</code>.</p>")
	@subsets
	@"http://www.omg.org/spec/SysML"
