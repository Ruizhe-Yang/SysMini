	@"http://www.omg.org/spec/SysML"
	volatile transient derived ref Type[*] featuringType;

	@GenModel(documentation="<p>The <code>ownedRelationships</code> of this <code>Feature</code> that are <code>TypeFeaturings</code> and for which the <code>Feature</code> is the <code>featureOfType</code>.</p>")
	@subsets
	@"http://www.omg.org/spec/SysML"
	volatile transient derived ref TypeFeaturing[*]#owningFeatureOfType ownedTypeFeaturing;

