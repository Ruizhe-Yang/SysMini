	@GenModel(documentation="<p>The <code>FeatureMembership</code> that owns this <code>Feature</code> as an <code>ownedMemberFeature</code>, determining its <code>owningType</code>.</p>")
	@subsets
	@"http://www.omg.org/spec/SysML"
