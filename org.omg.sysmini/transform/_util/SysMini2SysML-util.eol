/* AnnotatingElement */
operation Model!Comment Comment() : String {
	var str = "";
	if (self.declaredName != null and self.declaredName != ""){
		str += "comment".space();
		str += self.declaredName.space(); 
	}
	if (self.annotation.size() > 0){
		str += "about".space();
		for (annotationString in self.annotation){
			str += annotationString.space();
			// TBD
		}
	}
	if (self.locale != null){
		str += "locale".space() + self.locale.space();
	}
	str += "/";
	str += "*".space() + self.text;
	str += "*/".space();
	return str;
}

operation Model!Documentation Documentation() : String {
	var str = "";
	str += "doc".space();
	str += self.declaredName.space(); 
	if (self.locale != null){
		str += "locale".space() + self.locale.space();
	}
	str += "/";
	str += "*".space() + self.text;
	str += "*/".space();
	return str;
}

operation Model!TextualRepresentation TextualRepresentation() : String {
	var str = "";
	if (self.declaredName != null){
		str += "rep".space();
		str += self.declaredName.space(); 
	}
	if (self.language != null){
		str += "language".space() + self.language.space();
	}
	str += "/";
	str += "*".space() + self.text;
	str += "*/".space();
	return str;
}


